<div class="row">
    <div class="col-md-12">
        <h2>Images</h2>
        <hr />
    </div>

    {{#if images}}
        {{#each images}}
            <div class="col-md-4">
                <img src="https://res.cloudinary.com/seneca-web322/image/upload/{{this}}" class="img-responsive img-thumbnail" />
            </div>
        {{/each}}
    {{else}}
        <div class="col-md-12 text-center">
            <strong>No Images Available</strong>
        </div>
    {{/if}}
</div>

